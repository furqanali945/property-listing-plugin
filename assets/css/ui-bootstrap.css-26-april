.clearfix { clear: both; margin: 0; padding: 0; }
.main-contents {max-width: 1150px;margin: 0 auto;}

ul.width-50{margin:0 !important;padding:0 !important}
ul.width-50 li { width: 49%; float: left; margin: 0 0 15px 0; position: relative; list-style: none !important; }
ul.width-50 li:nth-child(2n) { float: right; }
ul.width-50 li img { position: relative; }
.img-gradient {position: relative;}
ul.width-33 li { float: left; width: 32.5%; margin: 0 1.25% 15px 0; position: relative; list-style: none !important; }
ul.width-33 li:nth-child(3n+3) { margin-right: 0; }

