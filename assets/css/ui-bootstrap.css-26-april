/* The Modal (background) */
.modal { display: none; /* Hidden by default */ position: fixed; /* Stay in place */ z-index: 9999; /* Sit on top */ padding-top: 100px; /* Location of the box */ left: 0; top: 0; width: 100%; /* Full width */ height: 100%; /* Full height */ overflow: auto; /* Enable scroll if needed */ background-color: rgb(0, 0, 0); /* Fallback color */ background-color: rgba(0, 0, 0, 0.4); /* Black w/ opacity */ }

/* Modal Content */
.modal-content { background-color: #fefefe; margin: auto; padding: 20px; border: 1px solid #888; max-width: 300px; text-align: center; }
.modal-content h3 { font-size: 16px; font-weight: 700; margin: 10px 0; text-align: center; }
.modal-content a { display: block; color: #fff; margin-top: 5px; font-size: 20px; font-weight: 700; text-align: center; border-radius: 2px; padding: 4px 10px; background: #e20031; letter-spacing: 1px; text-decoration: none; }
.modal-content h2 { margin: 15px 0; font-size: 13px; text-transform: uppercase; font-weight: 700; text-decoration: underline; text-align: left; }
.modal-content p { font-size: 14px; color: #000; margin-bottom: 8px; text-align: left; margin: 0 0 10px; }
.modal-content  img { width: 200px; height: 200px; background: #FFF; border-radius: 6px; }

/* The Close Button */
.close { color: #aaaaaa; float: right; font-size: 28px; font-weight: bold; }
.close:hover, .close:focus { color: #000; text-decoration: none; cursor: pointer; }
.inner-box { border: 1px solid #dadada; Hover over me
-webkit-transition: all 500ms ease-in-out; -moz-transition: all 500ms ease-in-out; -ms-transition: all 500ms ease-in-out; -o-transition: all 500ms ease-in-out; transition: all 500ms ease-in-out; }
.inner-box:hover { border: 1px solid #000; -webkit-transition: all 500ms ease-in-out; -moz-transition: all 500ms ease-in-out; -ms-transition: all 500ms ease-in-out; -o-transition: all 500ms ease-in-out; transition: all 500ms ease-in-out; }
.inner-box-img { padding: 10px; }
.grey-area { background: #ccc; padding: 5px; }
.grey-area a { width: 32.5%; height: 35px; float: left; font-size: 23px; line-height: 35px; border-radius: 0; margin: 0 1.25% 0 0; background: #FFFFFF; text-align: center; padding: 0px !important; border-color: transparent !important; }
.inner-box a:nth-child(3n) { margin-right: 0px; }
.inner-box a i.fa-phone { color: #e04006; }
.inner-box a i.fa-envelope { color: #e04006; }
.inner-box a i.fa-whatsapp { color: #25d366; }

/****** shortcode css ***********/
label.sold { text-align: center !important; color: #fff !important; background-color: red; padding: 10px; margin-bottom: 10px !important; display: inline-block; border-radius: 5px; }

ul.featured-developers li {float: left;width: 15%;margin-right: 1.4%;position: relative;border-radius: 2px;margin-bottom: 15px;min-height: 154px;border: 1px solid transparent;list-style-type: none;-webkit-box-shadow: 0 1px 3px rgba(0, 0, 0, .24);box-shadow: 0 1px 3px rgba(0, 0, 0, .24);}
ul.featured-developers li a { float: left; width: 100%; padding: 2px; }
ul.featured-developers { width: 100%; padding: 0; margin: 0; }
ul.featured-developers li img { padding: 2px; margin: 0 auto 0 auto !important; text-align: center; display: block; }


@media screen and (max-width:1022px) {
    .project-metas table.project-table > tbody > tr > td { width: 52%; }
}
@media screen and (max-width:991px) {
    ul.width-33 li { width: 49%; float: left; margin: 0 0 15px 0; }
    ul.width-33 li:nth-child(2) { margin-right: 0 !important; }
    ul.width-33 li:nth-child(2n+2) { float: right; }
    .lhs, .rhs { width: 100%; float: none; margin: 0 0 15px; }
}
@media screen and (max-width:767px) {
    .project-metas table.project-table > tbody > tr { width: 100%; }
    .search-area select, .search-area .search { width: 48%; margin: 0 2% 15px 0; }
}
@media screen and (max-width:650px) {
    ul.width-33 li, ul.width-50 li, ul.width-33 li:nth-child(2n+2), ul.width-50 li:nth-child(2n), ul.width-33 li:nth-child(2) { width: 90%; float: none; margin: 0 auto 15px !important; }
    .img-gradient img { width: 100% }
}
@media screen and (max-width:480px) {
    .search-area select, .search-area .search { width: 100%; margin: 0 0 15px 0; }
}






